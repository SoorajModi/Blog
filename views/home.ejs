<%- include('partials/header'); -%>
  <h1>Home</h1>
  <p><%= homeContent %></p>
  <hr>
  <h1>Blog Posts</h1>
  <% posts.forEach((post) => { %>
    <h2><%= post.title %></h2>
    <h5><%= post.date.toISOString().replace(/T/, ' ').replace(/\..+/, '')  %></h5>
    <p><%= post.body.substring(0, 100) + "..." %><a href="/posts/<%= post.title %>">Read More</a></p>
  <% }); %>
<%- include('partials/footer'); -%>
