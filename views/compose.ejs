<%- include('partials/header'); -%>
  <h1>Compose</h1>
  <form class="" action="/compose" method="post">
    <div class="form-group">
      <label for="composeTitle">Title</label>
      <input class="form-control" type="text" name="composeTitle">
      <label for="composeBody">Body</label>
      <textarea class="form-control" name="composeBody" rows="8" cols="80"></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="button">Publish</button>
  </form>
<%- include('partials/footer'); -%>
